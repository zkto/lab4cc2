    "def borde_igual(m):\n",
    "    m[:,-1][0] = m[0][0]\n",
    "    m[-1,:][0] = m[0][0]\n",
    "    m[0,:][-1] = m[0][0]\n",
    "    m[:,-1] = m[:,0]\n",
    "    m[0,:] = m[-1,:]\n",
    "    return m"
